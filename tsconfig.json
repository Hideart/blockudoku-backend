{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
        "@/*": ["src/*"],
        "@admin/*": ["src/app/super-admin/*"],
        "@ch/*": ["src/app/channel-partner/*"],
        "@dispatcher/*": ["src/app/dispatcher/*"]
    }
  },
  "include": [
    "src"
  ]
}
